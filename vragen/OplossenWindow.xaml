<Window x:Class="ProjectChallenge.OplossenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="OplossenWindow" Height="300" Width="300">
    <Grid>
        <TextBlock x:Name="opgaveTextBlock" HorizontalAlignment="Left" Margin="51,39,0,0" TextWrapping="Wrap" Text="Fout bij inlezen vragen" VerticalAlignment="Top"/>
        <TextBox x:Name="invulTextBox" HorizontalAlignment="Left" Height="23" Margin="51,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="volgendeButton" Content="Volgende Vraag" HorizontalAlignment="Left" Margin="10,188,0,0" VerticalAlignment="Top" Width="99" Click="volgendeButton_Click"/>
        <Button x:Name="klaarButton" Content="Klaar" HorizontalAlignment="Left" Margin="209,188,0,0" VerticalAlignment="Top" Width="75" Click="klaarButton_Click"/>
        <Button x:Name="vorigeButton" Content="Vorige Vraag" HorizontalAlignment="Left" Margin="10,212,0,0" VerticalAlignment="Top" Width="99" Click="vorigeButton_Click"/>
        <ListBox x:Name="invulListBox" HorizontalAlignment="Left" Height="100" Margin="51,83,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
